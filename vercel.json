{
  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "https://www.homesteadwestlasvegas.com/sitemap.xml",
      "permanent": true
    },
    {
      "source": "/robots.txt",
      "destination": "https://www.homesteadwestlasvegas.com/robots.txt",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
